        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Draw sprite depending on player state
if (!attacking) {
    switch (state) {
        case IDLE: 
            image_speed = 0.2;
            
            if (blocking)
            if  global.hummer = true{
                sprite_index = sPlayerIdleShieldHummer;
            }else{
                sprite_index = sPlayerIdleShield;
            }

            else
            if  global.hummer = true{
                sprite_index = sPlayerIdleHummer;
            }else{
                sprite_index = sPlayerIdle;
            }

        break;
        
        case RUN: 
            image_speed = 0.33; 
            
            if (blocking)
            if  global.hummer = true{
                sprite_index = sPlayerRunShieldHummer;
            }else{
                sprite_index = sPlayerRunShield;
            }

            else
            if  global.hummer = true{
                sprite_index = sPlayerRunHummer;
            }else{
                sprite_index = sPlayerRun;
            }

        break;
        
        case JUMP:
            // Mid jump   
            if (!(place_meeting(x, y + 2, oParSolid) &amp;&amp; vy != 0) &amp;&amp; vy &gt;= -1.0 &amp;&amp; vy &lt;= 1.0) {  
                if (blocking)
                if  global.hummer = true{
                    sprite_index = sPlayerJumpMShieldHummer;
                }else{
                    sprite_index = sPlayerJumpMShield;
                }

                else
                if  global.hummer = true{
                    sprite_index = sPlayerJumpMHummer;
                }else{
                    sprite_index = sPlayerJumpM; 
                }
 
           } else { 
                // Rise + fall
                if (vy &lt;= 0) {
                    if (blocking)
                    if  global.hummer = true{
                        sprite_index = sPlayerJumpUShieldHummer;
                    }else{
                        sprite_index = sPlayerJumpUShield;
                    }
 
                    else 
                    if  global.hummer = true{
                        sprite_index = sPlayerJumpUHummer;
                    }else{
                        sprite_index = sPlayerJumpU; 
                    }
   
                } else {
                    if (blocking)
                    if  global.hummer = true{
                        sprite_index = sPlayerJumpDShieldHummer;
                    }else{
                        sprite_index = sPlayerJumpDShield; 
                    }
                        
                    else
                    if  global.hummer = true{
                        sprite_index = sPlayerJumpDHummer;
                    }else{
                        sprite_index = sPlayerJumpD; 
                    }
                        
                }
            }
             
            // When against a wall   
            if (cRight || cLeft)
                if (blocking)
                    if  global.hummer = true{
                        sprite_index = sPlayerSlideShieldHummer;
                    }else{
                        sprite_index = sPlayerSlideShield; 
                    }
                    
                else
                    if  global.hummer = true{
                        sprite_index = sPlayerSlideHummer;
                    }else{
                        sprite_index = sPlayerSlide;  
                    }
                     
        break;
        
        case ROLL:
            // Don't do stuff here    
        break;
    }
}

// Draw player
if (onGround)
    draw_sprite_ext(sprite_index, image_index, x, y + (16 - 16 * yscale) * 0.25, facing * xscale, yscale, 0, c_white, image_alpha);    
else
    draw_sprite_ext(sprite_index, image_index, x, y, facing * xscale, yscale, 0, c_white, image_alpha);

</string>
          </argument>
        </arguments>
      </action>
